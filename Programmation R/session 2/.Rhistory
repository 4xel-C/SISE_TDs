# 8. Méthode de Simpson
simpson <- function(a, b, FONCTION, ...) {
fa <- FONCTION(a, ...)
fm <- FONCTION(m/2, ...)
fb <- FONCTION(b, ...)
membre1 <- (b-a)/6
membre2 <- fa + (4 * fm) + fb
return(membre1 * membre2)
}
